{
  "code": "de",
  "name": "hello_world",
  "text": "Hello World",
  "author": "",
  "active": true
}

/entries ->
  # Finds all translations as primary entry by primary language.
  TranslationDAO.findAllByCode("en") -> [
    {
      "code": "en",
      "name": "hello_world",
      "text": "Hello World"
      "active": true
    },
    {
      "code": "en",
      "name": "bye_bye",
      "text": "Bye Bye"
      "active": true
    }
  ]

/entries/hello_world ->
  # Finds all translations with the same name and for all languages.
  TranslationDAO.findAllByName("hello_world") -> [
    {
      "code": "en",
      "name": "hello_world",
      "text": "Hello World"
      "active": true
    },
    {
      "code": "de",
      "name": "hello_world",
      "text": "Hallo Welt"
      "active": true
    },
    {
      "code": "de",
      "name": "hello_world",
      "text": "Hallo Welt"
      "active": false
    }
  ]

